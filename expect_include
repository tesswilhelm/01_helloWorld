#!/usr/bin/expect --
set redConsole "\033\[01;31m"
set greenConsole "\033\[01;32m"
set yellowConsole "\033\[01;33m"
set resetConsole "\033\[01;0m"
set failedExit "[set redConsole]Program failed tests $resetConsole\n"
set passExit "[set greenConsole]Program passed tests $resetConsole\n"
set timeoutExit "[set redConsole]Program timed out $resetConsole\n"
set unexpectedExit "[set yellowConsole]Program delivered unexpected behavior $resetConsole\n"